{
  "name": "gatsby-source-microcms",
  "version": "3.0.0",
  "description": "Source plugin for Gatsby from microCMS.",
  "license": "MIT",
  "contributors": ["shibe97", "shooontan"],
  "repository": {
    "type": "git",
    "url": "https://github.com/microcmsio/gatsby-source-microcms.git"
  },
  "keywords": ["gatsby", "gatsby-plugin", "gatsby-source-plugin", "microCMS"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./gatsby-node": "./dist/gatsby-node.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["gatsby-node.js", "dist/"],
  "scripts": {
    "clean": "rimraf dist",
    "format": "biome format ./",
    "format:fix": "biome format --write ./",
    "lint": "biome lint ./",
    "lint:fix": "biome lint --apply ./",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "build": "tsup"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@vitest/coverage-v8": "^1.4.0",
    "gatsby": "^5.13.3",
    "microcms-js-sdk": "^3.1.0",
    "msw": "^2.2.13",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "camelcase": "^8.0.0"
  },
  "peerDependencies": {
    "gatsby": ">=5.0.0",
    "microcms-js-sdk": "^3.0.0"
  }
}
