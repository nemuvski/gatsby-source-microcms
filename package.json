{
  "name": "gatsby-source-microcms",
  "version": "3.0.0",
  "description": "Source plugin for Gatsby from microCMS.",
  "license": "MIT",
  "contributors": [
    "shibe97",
    "shooontan"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/microcmsio/gatsby-source-microcms.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "microCMS"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./gatsby-node": "./dist/gatsby-node.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "gatsby-node.js",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.js .",
    "format": "prettier --check './**/*.{js,ts}'",
    "format:fix": "prettier --write './**/*.{js,ts}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "build": "tsup"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-regexp": "^1.15.0",
    "gatsby": "^5.11.0",
    "microcms-js-sdk": "^2.5.0",
    "msw": "^1.2.2",
    "prettier": "^3.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "camelcase": "^6.3.0"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "microcms-js-sdk": "^2.0.0"
  }
}
