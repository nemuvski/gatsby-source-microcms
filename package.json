{
  "name": "gatsby-source-microcms",
  "version": "3.0.0",
  "description": "Source plugin for Gatsby from microCMS.",
  "license": "MIT",
  "contributors": ["shibe97", "shooontan"],
  "repository": {
    "type": "git",
    "url": "https://github.com/microcmsio/gatsby-source-microcms.git"
  },
  "keywords": ["gatsby", "gatsby-plugin", "gatsby-source-plugin", "microCMS"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./gatsby-node": "./dist/gatsby-node.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["gatsby-node.js", "dist/"],
  "scripts": {
    "clean": "rimraf dist",
    "format": "biome format ./",
    "format:fix": "biome format --write ./",
    "lint": "biome lint ./",
    "lint:fix": "biome lint --apply ./",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "build": "tsup"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@vitest/coverage-v8": "^0.33.0",
    "gatsby": "^5.11.0",
    "microcms-js-sdk": "^2.5.0",
    "msw": "^1.2.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "camelcase": "^6.3.0"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "microcms-js-sdk": "^2.0.0"
  }
}
